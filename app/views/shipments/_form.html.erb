    <%= f.input :commodity %>
    <%= f.input :weight %>
    <%= f.input :miles_or_km %>
    <%= f.input :load_size %>
    <%= f.input :miles %>
    <%= f.input :kilometers %>
    <%= f.input :invoice_price %>
    <%= f.input :booking_fee %>
    <%= f.input :percent_deducted %>
    <%= f.input :total_company_expenses %>
