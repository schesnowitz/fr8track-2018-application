    <%= f.input :percent_coverted_to_dollars %>
    <%= f.input :driver_cents_pm %>
    <%= f.input :rate_after_booking_fee %>
    <%= f.input :agent_fee %>
    <%= f.input :pick_up_date %>
    <%= f.input :pick_up_time %>
    <%= f.input :delivery_date %>
    <%= f.input :delivery_time %>
    <%= f.input :delivery_notes %>
    <%= f.input :equipment_type %>
    <%= f.input :status_name %>
    <%= f.input :updated_by %>
    <%= f.input :special_instructions %>
    <%= f.input :dimentions %>
    <%= f.input :origin_street %>
    <%= f.input :origin_city %>
