<p id="notice"><%= notice %></p>

<h1>Kt Reports</h1>

<table class='table-striped table-bordered'>
  <thead>
    <tr>
      <th>Name</th>
      <th>ID</th>
      <th>Duty Status</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kt_reports.each do |data| %>


      <tr>
        <td><%= "#{data.user.first_name} #{data.user.last_name}" %></td>
        <td><%= data.user.id %></td>
        <td><%= data.user.duty_status %></td>
        <td><%= data.user.email %></td>
        <td><%= link_to 'Show', "#" %></td>
        <td><%= link_to 'Edit', "#" %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kt Report', new_kt_report_path %>


