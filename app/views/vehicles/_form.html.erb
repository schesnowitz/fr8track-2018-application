<%= simple_form_for(@vehicle) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :kt_vehicle_id %>
    <%= f.input :vin %>
    <%= f.input :year %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :fuel_type %>
    <%= f.input :plate_state %>
    <%= f.input :plate_number %>
    <%= f.input :odometer_units %>
    <%= f.input :edl_sn %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
